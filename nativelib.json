{
  "name": "gameanalytics",
  "description": "GameAnalytics plugin",
  "version": "0.1.0",
  "license": "MIT",
  "url": "https://github.com/DrMoriarty/godot-gameanalytics.git",
  "dependencies": [
  ],
  "files": {
    "scripts": "scripts"
  },
  "autoload": {
    "gameanalytics": "*res://scripts/gameanalytics.gd"
  },
  "variables": {
    "GameAnalytics/GAME_KEY": {
      "info": "Game key"
    },
    "GameAnalytics/SECRET_KEY": {
      "info": "Secret key"
    },
    "GameAnalytics/CustomDimensions01": {
      "info": "Available custom dimensions 01",
      "default": "ninja,samurai"
    },
    "GameAnalytics/CustomDimensions02": {
      "info": "Available custom dimensions 02",
      "default": "whale,dolphin"
    },
    "GameAnalytics/CustomDimensions03": {
      "info": "Available custom dimensions 03",
      "default": "horde,alliance"
    },
    "GameAnalytics/ResourceCurrencies": {
      "info": "Available resource currencies",
      "default": "gold,gems"
    },
    "GameAnalytics/ResourceItemTypes": {
      "info": "Available resource item types",
      "default": "boost,lives"
    }
  },
  "platform_ios": {
    "files": {
      "ios-framework/build/gameanalytics.gdnlib": "addons/gameanalytics-ios/",
      "ios-framework/build/gameanalytics.gdns": "addons/gameanalytics-ios/",
      "ios-framework/build/.gitignore": "addons/gameanalytics-ios/",
      "ios-framework/build/GodotGameAnalytics.xcframework": "addons/gameanalytics-ios/GodotGameAnalytics.xcframework"
    }
  },
  "platform_android": {
    "files": {
      "GA-SDK-GODOT/android_godot_3_2/downloads/GameAnalytics.gdap": "android/plugins/",
      "GA-SDK-GODOT/android_godot_3_2/downloads/GameAnalytics.release.aar": "android/plugins/"
    }
  }
}
